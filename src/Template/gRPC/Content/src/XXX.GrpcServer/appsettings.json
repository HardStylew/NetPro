{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Async", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Error",
        "Microsoft": "Debug",
        "System": "Debug",
        "System.Net.Http.HttpClient": "Debug",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "configure": [
          { "Name": "Console" }
        ]
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Netpro"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConsulOption": {
    "Enabled": true, //是否开启,不配置默认开启
    //"ServiceName": "xxx.grpc", //可留空；留空默认以入口程序名作为servicename
    "EndPoint": "http://192.168.100.187:8500",
    "WaitTime": 30,
    "Tags": [ "HUHU" ], //可留空
    "Datacenter": null, //可留空
    "Token": null //可留空
  }
}
